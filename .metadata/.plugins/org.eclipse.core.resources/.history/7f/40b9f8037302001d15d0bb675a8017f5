package helloworld;

public class s5char {

	public static void main(String[] args) {
		char mychar ='D';
		System.out.println("mychar "+ mychar);
		
		int i=mychar;
		System.out.println(i);
		
		char x = 69;
		System.out.println("char x "+ x);
		
		char mynewchar = '\u0044';
		System.out.println(mynewchar);
		
		char mynewch = '\u0049';
		System.out.println(mynewch);
	}
}
